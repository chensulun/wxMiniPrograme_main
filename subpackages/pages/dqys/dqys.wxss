page {
    height: 100%;
    overflow: hidden;
    align-items: center;
    position: relative;
}

.bgimg {
    width: 750rpx;
}

.box {
    width: 94%;
    height: 100%;
    position: absolute;
    top: 50px;
    left: 3%;
}

.line-1px {
    width: 340px;
    height: 2px;
    background-color: #eeeeee;
    margin: 0 auto;
}
.line-1px-y{
    width: 2px;
    height: 50px;
    background-color: #eeeeee;
}
.f1 {
    height: 200px;
    width: 100%;
    border-radius: 10px;
    box-shadow: rgba(0, 0, 0, 0.3) 0 0 20px;
    overflow: hidden;
    background-color: white;
}

.f1-1 {
    margin: 20px 0 20px 30px;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.f1-1-2 {
    background-color: #fecb70;
    width: 120px;
    height: 50px;
    border-radius: 25px 0 0 25px;
    line-height: 50px;
}
.f1-2{
    display: flex;
    text-align: center;
    justify-content: space-around;
    margin-top: 10px;
}
.f2{
    text-align: center;
}