/* pages/xzrwd/xzrwd.wxss */
page {
  height: 100%;
  background-color: #EDEDED;
}
.mb-10 {
  margin-bottom: 10px;
}
.pr-40 {
  padding-right: 50px !important;
}
.text-right {
  text-align: right;
}
.cell-container {
  position: relative;
}
.btn-gray {
  background-color: #ADADAD;
  color: #fff;
  border-radius: 14.5px;
}
.cell-icon {
  width: 20px;
  height: 20px;
  position: absolute;
  top: 15px;
  right: 16px;
  text-align: center;
}
.cell-icon-add {
  width: 20px;
  height: 20px;
  border: 1px solid #29C552;
  color: #29C552;
  line-height: 18px;
  border-radius: 50%;
  position: absolute;
  font-size: 18px;
  top: 15px;
  right: 16px;
  text-align: center;
}
.tpj-name {
  font-size: 11px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #191919;
  line-height: 16px;
  height: 30px;
  line-height: 30px;
  padding-left: 8px;
}
.xzrw-container {
  padding: 9px 10px;
}
.xzrw-title {
  height: 40px;
  background-color: #fff;
  border-radius: 20px;
}
.title-item {
  width: 33.33%;
  border-radius: 20px;
  text-align: center;
  font-size: 15px;
  line-height: 40px;
  color: #6F6A6A;
  float: left;
}
.title-item.active {
  background-color: #26C44F;
  color: #fff;
}
.cells-item {
  border-radius: 8px;
  overflow: hidden;
}
.basic-title {
  font-size: 15px;
  padding-left: 10px;
  margin-bottom: 10px;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(28,28,22,1);
  position: relative;
}

.weui-cell {
  font-size: 14px;
}
.btn-del {
  border: none;
  background-color: transparent;
  outline: none;
  color: #989595;
  font-size: 12px;
}
.btn-del:after {
  border: none;
}
.btn-add {
  position: absolute;
  top: -5px;
  right: 2px;
  border: none;
  background-color: transparent;
  outline: none;
  color: #989595;
  font-size: 12px;
}
.btn-add:after {
  border: none;
}
.weui-icon {
  vertical-align: -1px !important;
}
/* 绑定成功弹窗 */
.modal-success {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.31);
  z-index: 999;
  display: none;
}
.modal-success.show {
  display: inline-block;
}
.modal-content {
  width: 270px;
  background: #FFFFFF;
  border-radius: 4px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -70%);
}
.tips-content {
  box-sizing: border-box;
  height: 114px;
  padding-top: 20px;
}
.tips-icon {
  width: 40px;
  height: 40px;
  border-radius: 50%;
  margin: 0px auto 10px;
  background-color: #87D068;
  text-align: center;
  line-height: 40px;
}
.tips-text {
  text-align: center;
  font-size: 18px;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 18px;
}
.modal-btns button {
  width: 50%;
  height: 50px;
  line-height: 50px;
  background-color: #fff;
  border: none;
  border-radius: 0px;
  border-top: 1px solid #DDDDDD;
}
.modal-btns button:after {
  border: none;
  border-radius: 0;
}
.btn-cancle {
  font-size: 18px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  border-right: 1px solid #ddd !important;
}
.btn-confirm {
  font-size: 18px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #26C44F;
}
/* 错误弹窗 */
.tips-info {
  padding: 30px 20px;
}
.tips-top {
  text-align: center;
  margin-bottom: 25px;
}
.error-icon {
  display: inline-block;
  width: 26px;
  height: 26px;
  background-color: #FFB460;
  line-height: 26px;
  color: #fff;
  border-radius: 50%;
  margin-right: 10px;
}
.error-text {
  display: inline-block;
  font-size: 17px;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
}
.error-name {
  font-size: 17px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FF0000;
  line-height: 24px;
}
.error-txt {
  font-size: 17px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333;
  line-height: 24px;
}