/* subpackages/pages/dqys/dqys.wxss */
page{
    height: 100%;
    background-color: #5f5f5f;
    overflow: hidden;
    display: flex;
    justify-content: center;
    align-items: center;
}
.my-tip{
    height: 400px;
    width: 350px;
    background-color: white;
    border-radius: 10px;
    overflow: hidden;
    text-align: center;
}
.title{
    margin: 20px 0;
}
.detail{
    padding: 10px;
    text-align: left;
    width: 90%;
    height: 64%;
    margin: 0 auto;
    background-color: #EDEDED;
    overflow: hidden;
    overflow-y: scroll;
}
.my-btn{
    margin: 12px;
}