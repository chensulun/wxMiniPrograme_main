/* pages/rwdglnew/rwdglnew.wxss */

.page-rwdgl image {
    width: 100%;
    height: 100%;
}

.page-rwdgl {
    box-sizing: border-box;
}

.rwdgl-top {
    height: 158px;
    box-shadow: 0px 2px 1px 0px rgba(177, 176, 176, 0.16);
    border-top: 1px solid #EDEDED;
}

.rwdgl-add {
    width: 50%;
    height: 100%;
    border-right: 1px solid #EDEDED;
    float: left;
    box-sizing: border-box;
}

.rwdgl-search {
    width: 50%;
    height: 100%;
    float: left;
    box-sizing: border-box;
}

.rwd-add-icon {
    width: 68px;
    height: 66px;
    margin: 12px auto 16px;
}

.rwd-search-icon {
    width: 65px;
    height: 60px;
    margin: 15px auto 20px;
}

.rwd-title {
    text-align: center;
    font-size: 15px;
    color: #191919;
}

.rwd-subtitle {
    text-align: center;
    font-size: 12px;
    color: #919090;
}


