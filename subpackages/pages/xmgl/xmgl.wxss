/* pages/rwdgl/rwdgl.wxss */
.page-rwdgl image {
  width: 100%;
  height: 100%;
}
.page-rwdgl {
  box-sizing: border-box;
}
.rwdgl-top {
  height: 158px;
  box-shadow:0px 2px 1px 0px rgba(177,176,176,0.16);
  border-top: 1px solid #EDEDED;
}
.rwdgl-add {
  width: 33%;
  height: 100%;
  border-right: 1px solid #EDEDED;
  float: left;
  box-sizing: border-box;
}
.rwdgl-search {
  width: 33%;
  height: 100%;
  float: left;
  box-sizing: border-box;
}
.rwd-add-icon {
  width: 68px;
  height: 66px;
  margin: 12px auto 16px;
}
.rwd-search-icon {
  width: 65px;
  height: 60px;
  margin: 15px auto 20px;
}
.rwd-title {
  text-align: center;
  font-size: 15px;
  color: #191919;
}
.rwd-subtitle {
  text-align: center;
  font-size: 12px;
  color: #919090;
}
 /* 任务单供应量排行榜 */
.rwdgl-content {
  border-top: 16px solid #ededed;
}
.content-title {
  line-height: 30px;
  border-bottom: 1px solid #EDEDED;
  padding: 0 14px;
  font-size:15px;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(25,25,25,1);
}
.content-item {
  border-bottom: 1px solid #EDEDED;
  padding: 12px 20px 12px 14px;
  color: #323232;
}
.content-item-top {
  font-size: 13px;
}
.content-item-middle {
  font-size: 14px;
}
.content-item-bottom {
  font-size: 12px;
  color: #242424;
}
