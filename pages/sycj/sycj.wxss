/* pages/sycj/sycj.wxss */
page {
  background-color: #F0F0F0;
  padding: 12px 10px 26px;
  box-sizing: border-box;
}
.sycj-header {
  padding: 18px 12px;
  background-color: #fff;
  border-radius:11px;
  box-sizing: border-box;
}
.header-item {
  display: inline-block;
  width: calc((100% - 24px) / 3);
  height:40px;
  background:rgba(235,246,237,1);
  border-radius:3px;
  line-height: 20px;
  font-size: 14px;
  color: #29C552;
  padding: 10px 0;
  box-sizing: border-box;
  margin-right: 12px;
  text-align: center;
}
.header-item:nth-child(3n) {
  margin-right: 0;
}
.header-item:nth-child(-n+3) {
  margin-bottom: 14px;
}
.basic-title {
  padding: 20px 0 10px;
}
.content {
  padding: 4px 0;
  background-color: #fff;
  border-radius:11px;
}
.content-item {
  border-bottom: 1px solid #EDEDED;
  padding: 14px 84px 14px 14px;
  box-sizing: border-box;
  position: relative;
}
.content-item:last-child {
  border-bottom: none;
}
.content-title {
  font-size:15px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:21px;margin-bottom: 5px;
}
.content-subtitle {
  font-size:13px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:18px;
}
.btns-container {
  position: absolute;
  top: 30px;
  right: 14px;
}