/* pages/cjxz/cjxz.wxss */
page {
  height: 100%;
  background-color: #EDEDED;
}
.xzrw-container {
  padding: 8px 10px;
}
.xzrw-title {
  height: 40px;
  background-color: #fff;
  border-radius: 20px;
}
.title-item {
  width: 33.33%;
  border-radius: 20px;
  text-align: center;
  font-size: 15px;
  line-height: 40px;
  color: #6F6A6A;
  float: left;
}
.title-item.active {
  background-color: #26C44F;
  color: #fff;
}
.cells-item {
  border-radius: 8px;
  overflow: hidden;
  margin-top: 8px;
}
.weui-cell__hd{
  /* min-width: 80px; */
  padding-right: 20px;
}